{
  "AWSTemplateFormatVersion": "2010-09-09",

  "Parameters": {
    "BucketName": {
      "Type": "String",
      "Default": "bucket-name"
    }
  },

  "Resources": {
    "CLOUDFORMATIONNP": {
      "Type": "AWS::S3::Bucket",
      "DeletionPolicy": "Delete",
      "Properties": {
        "BucketName": { "Ref": "BucketName" },
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        },
        "Tags": [
          { "Key": "old_tag1", "Value": "old_val1" },
          { "Key": "old_tag2", "Value": "old_val2" },
          {
           "Key": "new_tag",
           "Value": "new_value"
          },
          {
           "Key": "another_tag",
           "Value": "another_val"
          },
          {
           "Key": "yor_trace",
           "Value": "4caaaaa0-e1e8-48db-abd8-41af0383fe09"
          },
          {
           "Key": "yor_name",
           "Value": "CLOUDFORMATIONNP"
          }
        ]
      }
    }
  }
}
