AWSTemplateFormatVersion: 2010-09-09
Description: Test FMS Policy tags

Resources:
  testPolicy:
    Type: AWS::FMS::Policy
    Properties:
      Tags:
        - Key: "isSpecial"
          Value: "true"
        - Key: new_tag
          Value: new_value
        - Key: yor_trace
          Value: 449189aa-f0d6-4718-b67c-17e747d3bdfe
        - Key: yor_name
          Value: testPolicy
      ResourceType: AWS::ElasticLoadBalancingV2::LoadBalancer
